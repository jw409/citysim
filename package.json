{
  "name": "urbansynth",
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && npx vite-bundle-visualizer",
    "preview": "vite preview",
    "test": "playwright test",
    "test:fast": "npx playwright test tests/e2e/core/fast-initial-load.spec.ts --project=chromium",
    "test:fast-all": "npm run test:fast && npm run test:core",
    "test:core": "node scripts/test-runner.cjs core",
    "test:viewport": "node scripts/test-runner.cjs viewport",
    "test:simulation": "node scripts/test-runner.cjs simulation",
    "test:ui": "node scripts/test-runner.cjs ui-panels",
    "test:visual": "node scripts/test-runner.cjs visual",
    "test:headed": "node scripts/test-runner.cjs headed all",
    "test:debug": "node scripts/test-runner.cjs debug core",
    "test:cross-browser": "node scripts/test-runner.cjs cross-browser",
    "test:smoke": "node scripts/test-runner.cjs smoke",
    "test:performance": "node scripts/test-runner.cjs performance",
    "test:report": "node scripts/test-runner.cjs report",
    "test:install": "npx playwright install",
    "test:update-snapshots": "node scripts/test-runner.cjs update-snapshots",
    "lint": "eslint src --ext ts,tsx --max-warnings 50",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install || true",
    "prebuild": "npm run build:city && npm run build:wasm",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir ../src/wasm",
    "build:city": "node scripts/generate_city.cjs",
    "build:city:geographic": "node scripts/generate_geographic_city.cjs",
    "build:city:manhattan": "node scripts/generate_geographic_city.cjs manhattan",
    "build:city:san-francisco": "node scripts/generate_geographic_city.cjs san_francisco",
    "build:city:denver": "node scripts/generate_geographic_city.cjs denver",
    "build:city:miami": "node scripts/generate_geographic_city.cjs miami",
    "build:city:seattle": "node scripts/generate_geographic_city.cjs seattle",
    "build:city:chicago": "node scripts/generate_geographic_city.cjs chicago",
    "build:city:las-vegas": "node scripts/generate_geographic_city.cjs las_vegas",
    "build:city:new-orleans": "node scripts/generate_geographic_city.cjs new_orleans",
    "build:city:custom": "node scripts/generate_geographic_city.cjs custom",
    "deploy": "npm run build && node scripts/deploy-pages.js",
    "protoc": "npx pbjs -t static-module -w commonjs -o src/data/city_model.js src/data/city_model.proto && npx pbts -o src/data/city_model.d.ts src/data/city_model.js"
  },
  "dependencies": {
    "@deck.gl/aggregation-layers": "^9.1.14",
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/react": "^9.0.0",
    "playwright": "^1.55.1",
    "protobufjs": "^7.5.4",
    "puppeteer": "^24.22.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.160.0",
    "zetta-mcp-tools": "file:../game1/ZMCPTools"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/protobufjs": "^6.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "protobufjs-cli": "^1.1.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "terser": "^5.44.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-bundle-visualizer": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
