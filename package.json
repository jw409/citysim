{
  "name": "urbansynth",
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prebuild": "npm run build:city && npm run build:wasm",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir ../src/wasm",
    "build:city": "node scripts/generate_city.cjs",
    "protoc": "npx pbjs -t static-module -w commonjs -o src/data/city_model.js src/data/city_model.proto && npx pbts -o src/data/city_model.d.ts src/data/city_model.js"
  },
  "dependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/react": "^9.0.0",
    "protobufjs": "^7.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/protobufjs": "^6.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
